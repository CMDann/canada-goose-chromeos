{
  "manifest_version": 2,

  "name": "SysInfo",
  "description": "Monitor your system, CPU, and display information!",
  "version": "2.2",

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "logo.png", "128": "logo.png" },

  "permissions": [
    "http://*/",
    "https://*/",
    "system.cpu",
    "system.memory",
    "system.storage",
    "system.display",
    "storage"
  ]
}
